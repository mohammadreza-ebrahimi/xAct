 RowBox[{
  RowBox[{"SymmetryOfFreeIndices", "[", 
   RowBox[{"expr_", ",", "SpinVBs_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"freeinds", "=", 
       RowBox[{"FindFreeIndices", "@", "expr"}]}], ",", " ", 
      "\[IndentingNewLine]", "freeinvbslots", ",", " ", "restslots", ",", " ",
