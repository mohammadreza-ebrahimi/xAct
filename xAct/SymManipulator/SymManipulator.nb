      "freefirstperm", ",", " ", "freesym", ",", "freeinvb", ",", 
      "restfrees"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"n", ",", "slotexpr", ",", "slotrules", ",", "sym"}], "}"}], 
      "=", 
      RowBox[{"List", "@@", 
       RowBox[{"SymmetryOf", "[", "expr", "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"freeinvb", "=", 
      RowBox[{"Select", "[", 
       RowBox[{"freeinds", ",", 
        RowBox[{
         RowBox[{"MemberQ", "[", 
          RowBox[{"SpinVBs", ",", 
           RowBox[{"UpIndex", "@", 
            RowBox[{"VBundleOfIndex", "[", "#", "]"}]}]}], "]"}], "&"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
