         RowBox[{
          RowBox[{"RInv", "[", 
           RowBox[{"_", ",", "_"}], "]"}], "|", 
          RowBox[{"DualRInv", "[", 
           RowBox[{"_", ",", "_"}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", "Infinity"}], "}"}]}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"inters", "=", 
      RowBox[{"Intersection", "[", 
       RowBox[{"right", ",", "knowns"}], "]"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"inters", "=!=", 
        RowBox[{"{", "}"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
