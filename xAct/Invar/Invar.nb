       "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Checked case \>\"", ",", "case"}], "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"knowns", "=", 
      RowBox[{"Join", "[", 
       RowBox[{"knowns", ",", 
        RowBox[{"First", "/@", "rules"}]}], "]"}]}]}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{"count", ",", "1", ",", 
      RowBox[{"Length", "[", "cases", "]"}]}], "}"}]}], 
