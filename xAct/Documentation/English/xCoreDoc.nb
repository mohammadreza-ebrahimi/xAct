  "General","warning",
   "\"\\!\\(\\*StyleBox[\\!\\(\\*RowBox[{\\\"\\\\\\\"Testing \
message\\\\\\\"\\\"}]\\),FontColor->RGBColor[0,0,1]]\\) : From \
\\!\\(\\*RowBox[{\\\"\\\\\\\"xCoreDoc\\\\\\\"\\\"}]\\).\"",2,22,1,
   19253587068399729852,"Local"},
