  RowBox[{
   RowBox[{"{", 
    RowBox[{"$WarningFrom", "=", "\"\<xCoreDoc\>\""}], "}"}], ",", 
   RowBox[{"Message", "[", 
    RowBox[{
     RowBox[{"General", "::", "warning"}], ",", "\"\<Testing message\>\"", 
     ",", "$WarningFrom"}], "]"}]}], "]"}]], "Input",
