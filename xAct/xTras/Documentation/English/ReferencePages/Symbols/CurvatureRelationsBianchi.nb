  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Del]", "d"], 
     SuperscriptBox[
      SubscriptBox["R", 
       StyleBox["abc",
        FontSlant->"Italic"]], "d"]}], "=", " ", 
    RowBox[{
     FormBox[
      RowBox[{
       RowBox[{
        SubscriptBox["\[Del]", "b"], 
        SubscriptBox["R", 
         StyleBox["ac",
          FontSlant->"Italic"]]}], "-", 
       RowBox[{
        SubscriptBox["\[Del]", "a"], 
        SubscriptBox["R", 
         StyleBox["bc",
          FontSlant->"Italic"]]}], "+", " ", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["R", 
          StyleBox["adc",
           FontSlant->"Italic"]], "e"], 
        SubscriptBox[
         SuperscriptBox["T", "d"], 
         StyleBox["be",
          FontSlant->"Italic"]]}], "-", " ", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["R", 
          StyleBox["bdc",
           FontSlant->"Italic"]], "e"], 
        SubscriptBox[
         SuperscriptBox["T", "d"], 
         StyleBox["ae",
          FontSlant->"Italic"]]}]}],
      TraditionalForm], "-", " ", 
     RowBox[{
      SubscriptBox["R", 
       StyleBox["dc",
        FontSlant->"Italic"]], 
      SubscriptBox[
       SuperscriptBox["T", "d"], 
       StyleBox["ab",
        FontSlant->"Italic"]]}]}]}], TraditionalForm]], "InlineMath"],
 " and the second contraction ",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    FormBox[
     RowBox[{
      RowBox[{
       SubscriptBox["\[Del]", "b"], 
       SuperscriptBox[
        SubscriptBox["R", "a"], "b"]}], "=", " ", 
