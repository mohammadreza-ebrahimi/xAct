     RowBox[{
      SubscriptBox["\[ContourIntegral]", "\[CapitalSigma]"], " ", 
      RowBox[{
       FractionBox[
        RowBox[{"\[Delta]", " ", "L"}], 
        RowBox[{"\[Delta]", " ", 
         SubscriptBox["R", 
          StyleBox["abcd",
           FontSlant->"Italic"]]}]], 
       SubscriptBox["\[Epsilon]", 
        StyleBox["ab",
         FontSlant->"Italic"]], 
       SubscriptBox["\[Epsilon]", 
        StyleBox["cb",
